{
  
  "$schema": "https://openapi.vercel.sh/vercel.json",
  // resto do seu conte√∫do aqui

  "version": 2,
  "builds": [
    {
      "src": "src/index.js",  // Ou "api/index.js" se renomear
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"  // Ou "api/index.js"
    }
  ]
}
